srcset=3D"https://secure.gravatar.com/avatar/8081b26e05bb4354f7d65ffc34cb=
bd67?s=3D400&amp;d=3Dmm&amp;r=3Dpg 2x">=20
           <SPAN class=3D"p-author">              Chris Coyier           =
 </SPAN> =20
         </A>           <SPAN class=3D"slashes">//</SPAN>           =
<time class=3D"dt-published"=20
datetime=3D"2014-09-02">            September 2, 2014          </time>   =
